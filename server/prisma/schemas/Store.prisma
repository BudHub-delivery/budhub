model Store{
  id          Int         @id @default(autoincrement())
  store_name  String      
  address_id  Int
  address     Address     @relation(fields: [address_id], references: [id])
  items       Item[]
  orders      Order[]
  store_roles Store_Role[]
  store_tax   Store_Tax?
  is_active   Boolean     @default(true)
}